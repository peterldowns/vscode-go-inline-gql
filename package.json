{
  "name": "vscode-go-inline-gql",
  "displayName": "Inline Graphql for go",
  "icon": "images/logo.png",
  "description": "Syntax highlighting for inline graphql in go",
  "version": "0.1.2",
  "license": "MIT",
  "publisher": "peterldowns",
  "engines": {
    "vscode": "^1.53.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peterldowns/vscode-go-inline-gql.git"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "path": "./syntaxes/go-gql.json",
        "scopeName": "graphql-string.injection",
        "injectTo": [
          "source.go"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.sql": "graphql"
        }
      }
    ]
  }
}
